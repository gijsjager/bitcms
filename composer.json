{
    "name": "gijsjager/bitcms",
    "description": "Bitcms plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.5.*",
        "holt59/cakephp3-bootstrap-helpers": "dev-cake4-bs4-1.0.0",
        "stefangabos/zebra_image": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Bitcms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitcms\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
